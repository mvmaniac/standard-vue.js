{
  "env": {
    "browser": true,
    "node": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": ["warn"],
    "no-underscore-dangle": ["warn"]
  }
}
