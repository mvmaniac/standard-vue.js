{
  "env": {
    "browser": true,
    "node": true,
    "es2020": true
  },
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["import", "prettier"],
  "rules": {
    "prettier/prettier": ["error"],
    "no-unused-vars": ["warn"],
    "no-underscore-dangle": ["warn"]
  }
}
