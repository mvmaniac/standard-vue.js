{
  "env": {
    "browser": true,
    "node": true,
    "es2022": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parser": "@babel/eslint-parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["import"],
  "rules": {
    "no-unused-vars": ["warn"],
    "no-underscore-dangle": ["warn"]
  }
}
